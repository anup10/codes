int main()
{
	char inp[26],inp1[30];
	cout<<"Entr the encryption:"<<endl;
	for(int ip=0;ip<26;ip++)
	{
		cin>>inp[ip];
	}
	cout<<"enter your text:"<<endl;
	int ip=0;
	cin>>inp1[ip];
	while(inp1[ip]!='\n')
	{
		ip++;
		cin>>inp1[ip];
		
	}
		
	int jk=0;
	while(inp1[jk]!= '\n')
	{
		cout<<inp[(inp1[jk])%26]<<endl;
		jk++;
	}
	return 0;
}


































